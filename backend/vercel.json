{
"version": 2,
"builds": [
{
"src": "server.js",
"use": "@vercel/node",
"config": {
"includeFiles": [
"dist/**"
]
}
}
],
"routes": [
{
"src": "/api/(.*)",
"dest": "server.js",
"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
"headers": {
        "Access-Control-Allow-Origin": "https://yostay.vercel.app, http://localhost:5173",
        "Access-Control-Allow-Origin": "https://yostay.vercel.app",
"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
"Access-Control-Allow-Headers": "Content-Type, Authorization",
"Access-Control-Allow-Credentials": "true"
}
},
{
"src": "/(.*)",
"dest": "server.js"
}
]
}
